@article{ ISI:000459349900002,
Author = {Pantzalis, Christos and Ucar, Erdem},
Title = {{What Happens When Mood Disagrees with Financial News? Investors
   Underreact to Firm News Conflicting with Mood}},
Journal = {{INTERNATIONAL JOURNAL OF BUSINESS}},
Year = {{2019}},
Volume = {{24}},
Number = {{1}},
Pages = {{25-54}},
Abstract = {{We use mood-generating events-proxied by big sports games-that contain
   no information on the firms' fundamentals but happen concurrently with
   earnings announcements to test the hypothesis that investors' attention
   shifts away from financial news that is incongruent with investors' mood
   states, thereby leading to underreaction. We empirically confirm the
   existence of conflicting-mood distractions. We find a stronger
   post-earnings announcement drift and delayed response ratio and weaker
   immediate volume reaction when the news (positive or negative) in a
   firm's earnings announcement is inconsistent with the mood generated by
   the win or loss of the investors' local sports team in a major game.
   This effect strengthens with a firm's proximity to the location of the
   mood source.}},

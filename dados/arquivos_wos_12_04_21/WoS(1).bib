@article{ ISI:000331594500002,
Author = {Andreff, Wladimir},
Title = {{Building Blocks for a Disequilibrium Model of a European Team Sports
   League}},
Journal = {{INTERNATIONAL JOURNAL OF SPORT FINANCE}},

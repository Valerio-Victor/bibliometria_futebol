@article{ ISI:000498160800003,
Author = {Chen, Yangyang and Dietl, Helmut M. and Orlowski, Johannes and Zheng,
   Fang},
Title = {{The Effect of Investment in European Football on the Market Value of
   Chinese Corporations}},
Journal = {{INTERNATIONAL JOURNAL OF SPORT FINANCE}},
Year = {{2019}},
Volume = {{14}},
Number = {{4}},
Pages = {{249-261}},
Month = {{NOV}},
Note = {{10th Conference of the European-Sports-Economics-Association, Liverpool,
   ENGLAND, AUG, 2019}},
Organization = {{European Sports Econ Assoc}},
Abstract = {{This paper analyzes how investments in professional European football
   clubs affect the market value of Chinese corporations. Twelve
   acquisition cases with complete stock return data are identified between
   2009 and 2017. Applying traditional financial event study methodology,
   it appears that Chinese corporations, on average, do not earn abnormal
   returns as a result of their investments in professional European
   football clubs. However, as exceptions to this finding, a few
   corporations show significant positive or negative abnormal returns.
   This study also hints at an association between the change in market
   value of acquiring corporations, the relative transaction size, and
   their industry relatedness. These findings have important implications
   for corporate and football managers, current stock holders, and future
   investors.}},

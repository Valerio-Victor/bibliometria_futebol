@article{ ISI:000514393400010,
Author = {Bauers, Sebastian Bjoern and Hovemann, Gregor},
Title = {{The regulation of investors' influence in German professional football.
   An analysis of the `50+1 Rule' and its future from the perspective of
   potential investors}},
Journal = {{GERMAN JOURNAL OF EXERCISE AND SPORT RESEARCH}},

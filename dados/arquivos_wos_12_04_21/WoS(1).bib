@article{ ISI:000505945700015,
Author = {Crisci, Anna and D'Ambra, Luigi and Esposito, Vincenzo},
Title = {{A Generalized Estimating Equation in Longitudinal Data to Determine an
   Efficiency Indicator for Football Teams}},
Journal = {{SOCIAL INDICATORS RESEARCH}},

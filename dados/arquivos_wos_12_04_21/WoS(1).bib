@article{ ISI:000372037900003,
Author = {Davies, Robert W. and Toomey, Clodagh and McCormack, William and Hughes,
   Katie and Cremona, Alexandra and Jakeman, Philip},
Title = {{Body composition analysis of inter-county Gaelic athletic association
   players measured by dual energy X-ray absorptiometry}},
Journal = {{JOURNAL OF SPORTS SCIENCES}},

@article{ ISI:000491475500004,
Author = {Alvarado-Vargas, Marcelo and Zou, Qi},
Title = {{The importance of good behavior in college football teams in the USA An
   interaction effect of team reputation on game attendance}},
Journal = {{INTERNATIONAL JOURNAL OF SPORTS MARKETING \& SPONSORSHIP}},

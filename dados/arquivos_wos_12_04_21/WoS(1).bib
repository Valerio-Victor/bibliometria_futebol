@article{ ISI:000459563700001,
Author = {Manoli, Argyro Elisavet and Antonopoulos, Georgios A. and Bairner, Alan},
Title = {{The inevitability of corruption in Greek football}},
Journal = {{SOCCER \& SOCIETY}},
Year = {{2019}},
Volume = {{20}},
Number = {{2}},
Pages = {{199-215}},
Abstract = {{From the late 1990s, corrupt practices in Greek football have been
   considered to pose a serious threat to the integrity of the sport, with
   a number of schemes and measures being introduced as a response. The aim
   of this article is to show why corruption in Greek football is
   inevitable by offering a detailed account of three football-related
   corrupt practices and highlighting their contextual parameters, as well
   as juxtaposing them against the set of measures that have been
   implemented. By placing corruption in football in the wider landscape of
   the country and of global football, and examining the political,
   structural and economic factors that contribute to the overall
   managerial and financial implications of corruption, we present the
   reader with the new norm which, in reality, makes corruption the only
   game in town'.}},

@article{ ISI:000392463700001,
Author = {Terrien, Mickael and Scelles, Nicolas and Durand, Christophe},
Title = {{French 75\% Tax Rate: An Opportunity to Optimize the Attractiveness of
   the French Soccer League}},
Journal = {{INTERNATIONAL JOURNAL OF SPORT FINANCE}},
Year = {{2016}},
Volume = {{11}},
Number = {{3}},
Pages = {{183-203}},
Month = {{AUG}},
Abstract = {{This paper analyzes the impact of the French 75\% income tax rate on the
   attractiveness of the French soccer league. The concerns are less about
   its financial implications for clubs than about the possible decrease in
   its attractiveness. A classical model of professional team sport leagues
   is employed to measure the Nash equilibrium competitive balance and the
   stock of talent to assess the effect of the new taxation. We then
   propose two hypotheses corresponding to specific situations in the
   French soccer league: ``social and fiscal disparities between clubs{''}
   and ``sugar daddy{''} behavior. The new model predicts a polarization of
   the league and an exodus of talent, which could be mitigated by revenue
   sharing.}},

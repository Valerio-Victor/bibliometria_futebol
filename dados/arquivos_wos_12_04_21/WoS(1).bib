@article{ ISI:000459369100001,
Author = {Curren, Randall and Blokhuis, J. C.},
Title = {{Friday Night Lights Out: The End of Football in Schools}},
Journal = {{HARVARD EDUCATIONAL REVIEW}},
Year = {{2018}},
Volume = {{88}},
Number = {{2}},
Pages = {{141-162}},
Month = {{SUM}},
Abstract = {{In this essay, ethicists grounded in philosophy (Curren) and law
   (Blokhuis) argue that the US public schools' sponsorship of tackle
   football is ethically indefensible and inconsistent with their
   educational aims. Their argument relies on three ethical principles and
   a growing body of evidence that many students who play football suffer
   traumatic brain injury and cognitive impairment that undermine their
   academic success and life prospects, whether or not they suffer
   concussions. The authors also address educational claims made on behalf
   of football, the legal principles governing custodial responsibilities
   of schools and parents, factors that limit the moral and legal
   significance of children's consent to participate in football programs,
   and evidence that sponsorship of football programs subjects educational
   institutions to unsustainable financial risk.}},

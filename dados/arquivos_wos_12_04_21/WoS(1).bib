@article{ ISI:000358265600005,
Author = {Mills, Brian M. and Salaga, Steven},
Title = {{Historical Time Series Perspectives on Competitive Balance in NCAA
   Division I Basketball}},
Journal = {{JOURNAL OF SPORTS ECONOMICS}},
Year = {{2015}},
Volume = {{16}},
Number = {{6, SI}},

@article{ ISI:000546052100004,
Author = {Gomez-Martinez, Raul and Marques-Bogliani, Camila and Paule-Vianez,
   Jessica},
Title = {{The profitability of algorithmic trading systems based on football
   sentiment}},
Journal = {{INTERNATIONAL SPORTS STUDIES}},
Year = {{2020}},
Volume = {{42}},
Number = {{1}},
Pages = {{33-46}},
Abstract = {{Behavioural finance has shown that investment decisions are the result
   of not just rational but also emotional brain processes. On the
   assumption that emotions affect financial markets, it would seem likely
   that football results might have a measurable effect on financial
   markets. To test this, this study describes three algorithmic trading
   systems based exclusively on the results of three top European football
   teams (Juventus, Bayern Munchen and Paris St Germain) opening long or
   short positions in the next market season of the futures market of the
   index of each country (MIB (Milano Italia Borsa), DAX (Deutscher Aktien
   Index) and CAC (Cotation Assistee en Continu). Depending on the outcome
   of the last game played a long position was taken after a victory and a
   short position after a draw or defeat. The results showed that the
   algorithmic systems were profitable in the case of Juventus and Bayern
   whereas in the case of PSG, the system was profitable, but in an inverse
   way. This study shows that investment strategies that take account of
   sports sentiment could have a profitable outcome.}},
Publisher = {{LOGOS VERLAG BERLIN GMBH}},
Address = {{COMENIUSHOF, GUBENER STR 47, BERLIN, 10243, GERMANY}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Gomez-Martinez, R (Corresponding Author), Univ Rey Juan Carlos Madrid, Madrid, Spain.
   Gomez-Martinez, Raul; Paule-Vianez, Jessica, Univ Rey Juan Carlos Madrid, Madrid, Spain.

@article{ ISI:A1991HB93100005,
Author = {NEWBY, RW and SIMPSON, S},
Title = {{PERSONALITY PROFILE OF NONSCHOLARSHIP COLLEGE FOOTBALL PLAYERS}},
Journal = {{PERCEPTUAL AND MOTOR SKILLS}},
Year = {{1991}},
Volume = {{73}},
Number = {{3, 2}},
Pages = {{1083-1089}},
Month = {{DEC}},
Abstract = {{This paper presents a profile of the mood states of nonscholarship
   football players as measured by the Profile of Mood States inventory.
   Analysis suggests that these 120 athletes did not conform to the
   ``iceberg profile{''} described for elite athletes, differing mainly in
   the areas of tension and anger. The athletes measured for this report
   indicated more anger than a sample of 60 football players attending an
   institution providing financial aid for participation in sports. Total
   mood scores were lower than those of college athletes on scholarship and
   high school athletes and nonathletes. Differences in mood states were
   also found between members of the offensive and defensive squads and
   between varsity and junior varsity players.}},

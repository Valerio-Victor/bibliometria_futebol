@article{ ISI:000167526000005,
Author = {Szymanski, S},
Title = {{Income inequality, competitive balance and the attractiveness of team
   sports: Some evidence and a natural experiment from English soccer}},
Journal = {{ECONOMIC JOURNAL}},
Year = {{2001}},
Volume = {{111}},
Number = {{469, F}},
Pages = {{F69-F84}},
Month = {{FEB}},
Abstract = {{This paper examines the relationship between financial inequality,
   competitive balance and attendance at English professional league
   soccer. It shows that while financial inequality among the clubs has
   increased, competitive balance has remained relatively stable and match
   attendance appears unrelated to competitive balance. A clearer test
   of;he relationship is suggested by comparison with FA Cup matches.
   Because income inequality is primarily driven by inter- rather than
   intra-divisional inequality, the FA Cup has been a much more unbalanced
   competition than the divisional championships. Attendance at FA Cup
   matches relative to the corresponding league matches has fallen over the
   last twenty years.}},

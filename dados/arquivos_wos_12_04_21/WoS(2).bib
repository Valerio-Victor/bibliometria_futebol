@article{ ISI:000303560200004,
Author = {Storm, Rasmus K. and Nielsen, Klaus},
Title = {{Soft budget constraints in professional football}},
Journal = {{EUROPEAN SPORT MANAGEMENT QUARTERLY}},
Year = {{2012}},
Volume = {{12}},
Number = {{2}},
Pages = {{183-201}},
Abstract = {{Professional football in Europe is characterized by persistent deficits
   and growing debts among the majority of the top league clubs. Despite
   these problems, the clubs have an abnormally high survival rate. This
   paper focuses on this apparent paradox and poses the question: Why do
   only very few European professional football clubs go out of business
   even though they operate chronically on the edge of financial collapse?
   The paper argues that the paradox can be explained by the fact that
   professional football clubs operate within soft budget constraints in a
   way which is similar to the role of large companies in socialist
   economies - a phenomenon which was first identified by the Hungarian
   Economist Janos Kornai.}},

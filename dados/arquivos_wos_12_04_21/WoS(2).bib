@article{ ISI:000250824600008,
Author = {Dietl, Helmut M. and Franck, Egon},
Title = {{Governance failure and financial crisis in German football}},
Journal = {{JOURNAL OF SPORTS ECONOMICS}},
Year = {{2007}},
Volume = {{8}},
Number = {{6}},
Pages = {{662-669}},
Month = {{DEC}},
Abstract = {{The peculiar German football governance structure may be well suited to
   prevent integrity problems resulting from multiple club ownership or
   from ownership by ``undesired{''} persons or entities. However, this
   effect comes at a price. In the vacuum of power generated within large
   member associations, residual rights of control are de facto allocated
   to representatives who do not hold residual claims. Because these
   representatives externalize substantial parts of the risk associated
   with investment decisions, they are particularly ill-suited for managing
   the business of professional football, which has been transformed into a
   ``gamble on success{''} by ever-increasing revenue differentials between
   winners and losers. At the some time, low accounting standards for
   members clubs, combined with ``soft{''} low enforcement, invite club
   representatives to hide their consumption on the job behavior until
   their clubs are insolvent.}},

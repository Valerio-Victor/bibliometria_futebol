@article{ ISI:000313554800007,
Author = {Cullen, Francis T. and Latessa, Edward J. and Jonson, Cheryl Lero},
Title = {{Assessing the Extent and Sources of NCAA Rule Infractions A National
   Self-Report Study of Student-Athletes}},
